ogr2ogr -f "GPKG" /home/osmdata/gpkg/military_EPSG4326.gpkg -nln military_EPSG4326_point -nlt POINT PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_point where military <> ''"
