ogr2ogr -f "GPKG" /home/osmdata/gpkg/aeroway_EPSG4326.gpkg -nln aeroway_EPSG4326_point -nlt POINT PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_point where aeroway <> ''"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/aeroway_EPSG4326.gpkg -nln aeroway_EPSG4326_line -nlt LINESTRING -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_linestring where aeroway <> ''"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/aeroway_EPSG4326.gpkg -nln aeroway_EPSG4326_polygon -nlt POLYGON -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_polygon where aeroway <> ''"
