ogr2ogr -f "GPKG" /mnt/HC_Volume_4982572/gpkg/highway_EPSG4326.gpkg -nln highway_EPSG4326_point -nlt POINT PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_point where highway <> ''"
ogr2ogr -f "GPKG" /mnt/HC_Volume_4982572/gpkg/highway_EPSG4326.gpkg -nln highway_EPSG4326_line -nlt LINESTRING -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_linestring where highway <> ''"
ogr2ogr -f "GPKG" /mnt/HC_Volume_4982572/gpkg/highway_EPSG4326.gpkg -nln highway_EPSG4326_polygon -nlt POLYGON -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_polygon where highway <> ''"
psql -d osmdatabase -c "DROP TABLE import.osm_point"
psql -d osmdatabase -c "DROP TABLE import.osm_linestring"
psql -d osmdatabase -c "DROP TABLE import.osm_polygon"
