ogr2ogr -f "GPKG" /home/osmdata/gpkg/water_EPSG4326.gpkg -nln water_EPSG4326_point -nlt POINT PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_point where water <> ''"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/water_EPSG4326.gpkg -nln water_EPSG4326_line -nlt LINESTRING -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_linestring where water <> ''"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/water_EPSG4326.gpkg -nln water_EPSG4326_polygon -nlt POLYGON -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_polygon where water <> ''"
psql -d osmdatabase -c "DROP TABLE import.osm_point"
psql -d osmdatabase -c "DROP TABLE import.osm_linestring"
psql -d osmdatabase -c "DROP TABLE import.osm_polygon"

cd /home/osmdata/pbf/
