psql -d osmdatabase -c 'ALTER TABLE import.osm_polygon RENAME COLUMN "natural" TO "natural_attribute";'
ogr2ogr -f "GPKG" /home/osmdata/gpkg/waterway_EPSG4326.gpkg -nln waterway_EPSG4326_point -nlt POINT PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_point where waterway <> ''"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/waterway_EPSG4326.gpkg -nln waterway_EPSG4326_line -nlt LINESTRING -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_linestring where waterway <> ''"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/waterway_EPSG4326.gpkg -nln waterway_EPSG4326_polygon -nlt POLYGON -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_polygon where waterway <> ''"
psql -d osmdatabase -c "DROP TABLE import.osm_point"
psql -d osmdatabase -c "DROP TABLE import.osm_linestring"
psql -d osmdatabase -c "DROP TABLE import.osm_polygon"

cd /home/osmdata/pbf/
./imposm import -mapping mapping.yml -cachedir /home/osmdata/impcache -overwritecache -srid 4326 -write -connection postgis://osmdata:*PASSWORD*@localhost/osmdatabase -read planet-latest_boundary.osm.pbf
