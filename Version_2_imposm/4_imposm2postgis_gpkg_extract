ogr2ogr -f "GPKG" /home/osmdata/gpkg/historic_EPSG4326.gpkg -nln historic_EPSG4326_point -nlt POINT PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_point where historic <> ''"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/historic_EPSG4326.gpkg -nln historic_EPSG4326_line -nlt LINESTRING -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_linestring where historic <> ''"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/historic_EPSG4326.gpkg -nln historic_EPSG4326_polygon -nlt POLYGON -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_polygon where historic <> ''"
