psql -d osmdatabase -c 'ALTER TABLE import.osm_polygon RENAME COLUMN "mtb:description" TO "mtb_description";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_polygon RENAME COLUMN "parking:condition" TO "parking_condition";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_polygon RENAME COLUMN "parking:lane" TO "parking_lane";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_polygon RENAME COLUMN "railway:track_ref" TO "railway_track_ref";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "natural" TO "natural_attribute";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_linestring RENAME COLUMN "natural" TO "natural_attribute";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_polygon RENAME COLUMN "natural" TO "natural_attribute";'
ogr2ogr -f "GPKG" /home/osmdata/gpkg/natural_EPSG4326.gpkg -nln natural_EPSG4326_point -nlt POINT PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_point where "natural_attribute" <> ''"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/natural_EPSG4326.gpkg -nln natural_EPSG4326_line -nlt LINESTRING -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_linestring where "natural_attribute" <> ''"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/natural_EPSG4326.gpkg -nln natural_EPSG4326_polygon -nlt POLYGON -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_polygon where "natural_attribute" <> ''"
psql -d osmdatabase -c "DROP TABLE import.osm_point"
psql -d osmdatabase -c "DROP TABLE import.osm_linestring"
psql -d osmdatabase -c "DROP TABLE import.osm_polygon"

cd /home/osmdata/pbf/
./imposm import -mapping mapping.yml -cachedir /home/osmdata/impcache -overwritecache -srid 4326 -write -connection postgis://osmdata:*PASSWORD*@localhost/osmdatabase -read planet-latest_address.osm.pbf
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "aerialway:capacity" TO aerialway_capacity;'
