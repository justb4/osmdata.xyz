./imposm import -mapping mapping.yml -cachedir /home/osmdata/impcache -overwritecache -srid 4326 -write -connection postgis://osmdata:*PASSWORD*@localhost/osmdatabase -read planet-latest_geological.osm.pbf
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "aerialway:capacity" TO aerialway_capacity;'
