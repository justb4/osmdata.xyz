ogr2ogr -f "GPKG" /home/osmdata/gpkg/office_EPSG4326.gpkg -nln office_EPSG4326_point -nlt POINT PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_point where office <> ''"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/office_EPSG4326.gpkg -nln office_EPSG4326_line -nlt LINESTRING -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_linestring where office <> ''"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/office_EPSG4326.gpkg -nln office_EPSG4326_polygon -nlt POLYGON -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_polygon where office <> ''"
psql -d osmdatabase -c "DROP TABLE import.osm_point"
psql -d osmdatabase -c "DROP TABLE import.osm_linestring"
psql -d osmdatabase -c "DROP TABLE import.osm_polygon"

