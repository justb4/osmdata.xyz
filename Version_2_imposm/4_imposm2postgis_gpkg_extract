ogr2ogr -f "GPKG" /home/osmdata/gpkg/address_EPSG4326.gpkg -nln address_EPSG4326_point -nlt POINT PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_point where \"addr\" <> '' or \"addr_housenumber\" <> '' or \"addr_housename\" <> '' or \"addr_flats\" <> '' or \"addr_conscriptionnumber\" <> '' or \"addr_street\" <> '' or \"addr_place\" <> '' or \"addr_postcode\" <> '' or \"addr_city\" <> '' or \"addr_country\" <> '' or \"addr_full\" <> '' or \"addr_hamlet\" <> '' or \"addr_suburb\" <> '' or \"addr_subdistrict\" <> '' or \"addr_district\" <> '' or \"addr_province\" <> '' or \"addr_state\" <> '' or \"addr_interpolation\" <> '' or \"addr_inclusion\" <> '' or \"addr_door\" <> '' or \"addr_unit\" <> '' or \"addr_floor\" <> '' or \"addr_block\" <> ''"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/address_EPSG4326.gpkg -nln address_EPSG4326_line -nlt LINESTRING -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_linestring where \"addr\" <> '' or \"addr_housenumber\" <> '' or \"addr_housename\" <> '' or \"addr_flats\" <> '' or \"addr_conscriptionnumber\" <> '' or \"addr_street\" <> '' or \"addr_place\" <> '' or \"addr_postcode\" <> '' or \"addr_city\" <> '' or \"addr_country\" <> '' or \"addr_full\" <> '' or \"addr_hamlet\" <> '' or \"addr_suburb\" <> '' or \"addr_subdistrict\" <> '' or \"addr_district\" <> '' or \"addr_province\" <> '' or \"addr_state\" <> '' or \"addr_interpolation\" <> '' or \"addr_inclusion\" <> '' or \"addr_door\" <> '' or \"addr_unit\" <> '' or \"addr_floor\" <> '' or \"addr_block\" <> ''"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/address_EPSG4326.gpkg -nln address_EPSG4326_polygon -nlt POLYGON -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_polygon where \"addr\" <> '' or \"addr_housenumber\" <> '' or \"addr_housename\" <> '' or \"addr_flats\" <> '' or \"addr_conscriptionnumber\" <> '' or \"addr_street\" <> '' or \"addr_place\" <> '' or \"addr_postcode\" <> '' or \"addr_city\" <> '' or \"addr_country\" <> '' or \"addr_full\" <> '' or \"addr_hamlet\" <> '' or \"addr_suburb\" <> '' or \"addr_subdistrict\" <> '' or \"addr_district\" <> '' or \"addr_province\" <> '' or \"addr_state\" <> '' or \"addr_interpolation\" <> '' or \"addr_inclusion\" <> '' or \"addr_door\" <> '' or \"addr_unit\" <> '' or \"addr_floor\" <> '' or \"addr_block\" <> ''"
