#This script imports each osm.pbf file into a postgis database and creates extracts. Afterwards, the tables are dropped.
#Each iterative process replaces colon (:) by underline (_) and the reserved keyword "natural by "natural_attribute". [See issue 18](https://github.com/michaelmgis/osmdata.xyz/issues/18).
#Don't forget to set the passwords for the database user
#Run this script as osmdata user
# mapping.yml file is provided


cd /home/osmdata/pbf/
./imposm import -mapping mapping.yml -cachedir /home/osmdata/impcache -overwritecache -srid 4326 -write -connection postgis://osmdata:*PASSWORD*@localhost/osmdatabase -read planet-latest_natural.osm.pbf
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "aerialway:capacity" TO aerialway_capacity;'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "aerialway:duration" TO "aerialway_duration";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "aerialway:detachable" TO "aerialway_detachable";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "aerialway:bubble" TO "aerialway_bubble";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "aerialway:heating" TO "aerialway_heating";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "aerialway:summer:access" TO "aerialway_summer_access";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "building:levels" TO "building_levels";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "building:architecture" TO "building_architecture";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "building:use" TO "building_use";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:housenumber" TO "addr_housenumber";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:housename" TO "addr_housename";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:flats" TO "addr_flats";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:conscriptionnumber" TO "addr_conscriptionnumber";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:street" TO "addr_street";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:place" TO "addr_place";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:postcode" TO "addr_postcode";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:city" TO "addr_city";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:country" TO "addr_country";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:full" TO "addr_full";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:hamlet" TO "addr_hamlet";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:suburb" TO "addr_suburb";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:subdistrict" TO "addr_subdistrict";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:district" TO "addr_district";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:province" TO "addr_province";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:state" TO "addr_state";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:interpolation" TO "addr_interpolation";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:inclusion" TO "addr_inclusion";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:door" TO "addr_door";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:unit" TO "addr_unit";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:floor" TO "addr_floor";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "addr:block" TO "addr_block";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "mtb:scale" TO "mtb_scale";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "mtb:scale:uphill" TO "mtb_scale_uphill";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "mtb:scale:imba" TO "mtb_scale_imba";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "mtb:description" TO "mtb_description";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "parking:condition" TO "parking_condition";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "parking:lane" TO "parking_lane";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_point RENAME COLUMN "railway:track_ref" TO "railway_track_ref";'
psql -d osmdatabase -c 'ALTER TABLE import.osm_linestring RENAME COLUMN "aerialway:capacity" TO aerialway_capacity;'
psql -d osmdatabase -c 'ALTER TABLE import.osm_linestring RENAME COLUMN "aerialway:duration" TO "aerialway_duration";'
