psql -d osmdatabase -c 'ALTER TABLE import.osm_polygon RENAME COLUMN "natural" TO "natural_attribute";'
ogr2ogr -f "GPKG" /home/osmdata/gpkg/shop_EPSG4326.gpkg -nln shop_EPSG4326_point -nlt POINT PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_point where shop <> ''"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/shop_EPSG4326.gpkg -nln shop_EPSG4326_line -nlt LINESTRING -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_linestring where shop <> ''"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/shop_EPSG4326.gpkg -nln shop_EPSG4326_polygon -nlt POLYGON -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_polygon where shop <> ''"
psql -d osmdatabase -c "DROP TABLE import.osm_point"
psql -d osmdatabase -c "DROP TABLE import.osm_linestring"
psql -d osmdatabase -c "DROP TABLE import.osm_polygon"

