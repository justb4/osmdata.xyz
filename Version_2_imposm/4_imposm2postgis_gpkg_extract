psql -d osmdatabase -c 'ALTER TABLE import.osm_polygon RENAME COLUMN "natural" TO "natural_attribute";'
ogr2ogr -f "GPKG" /home/osmdata/gpkg/power_EPSG4326.gpkg -nln power_EPSG4326_point -nlt POINT PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_point where power <> ''"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/power_EPSG4326.gpkg -nln power_EPSG4326_line -nlt LINESTRING -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_linestring where power <> ''"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/power_EPSG4326.gpkg -nln power_EPSG4326_polygon -nlt POLYGON -update PG:"host=localhost dbname=osmdatabase user=osmdata password=*PASSWORD*" -sql "select * from import.osm_polygon where power <> ''"
